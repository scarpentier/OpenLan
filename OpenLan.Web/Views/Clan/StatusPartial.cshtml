@model OpenLan.Web.Models.Clan

<h2>@Model.Name</h2>
<div class="row">
    <div class="col-md-3">
        <b>Members</b><br />
        @foreach (var member in Model.Members)
        {
            @member.UserName<br />
        }
    </div>
    <div class="col-md-6">
        <p>Your token is <code>@Model.Token</code></p>
    </div>
</div>